#include <iostream>
using namespace std;
int main(){
int sum;
int array[10];
int array1[10];
cout << "Enter ten integers\n";
for (int a = 0; a< 10; a++) 
{
cin >> array[a];
}
cout<<"Reverse order(skipping elements at index 3 and index 5)";
for (int a = 9; a>= 0; a--)
{
if (a==3||a==5) 
{
continue;
}
cout<< array[a]<<"\n";}
cout<<"\n";
cout<< "Enter 10 integers for sum purpose\n";
for (int b = 0;b< 10;b++)
{
cin >> array1[b];
if (b==3 ||b== 5)
{
continue;}
sum += array[b];
}
cout<<"Sum without values on position 3 and 5"<<sum<<"\n";
return 0;
}
